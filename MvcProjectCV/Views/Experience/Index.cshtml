@using MvcProjectCV.Models.Entity 
@model List<Experience>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Experience List</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Subtitle</th>
        <th>Date</th>
        <th>Delete</th>
        <th>Edit</th>
    </tr>
    @foreach(var x in Model)
            {
                <tr>
                    <td>@x.ID</td>
                    <td>@x.Title</td>
                    <td>@x.Subtitle</td>
                    <td>@x.Date</td>
                    <td><a href="/Experience/DeleteExp/@x.ID" class="btn btn-danger">Delete</a></td>
                    <td><a href="/Experience/GetExp/@x.ID" class="btn btn-primary">Edit</a></td>
                </tr>
            }
</table>
<a href="/Experience/AddExp/" class="btn btn-success">Add New Experience</a>

