@model MvcProjectCV.Models.Entity.SocialMedia

@{
    ViewBag.Title = "GetPage";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h4>Edit Social Media</h4>
<br />
<form class="form-group" method="post" onsubmit="showMessage()">
    @Html.Label("Social Media Name")
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    @Html.Label("Social Media Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Social Media Icon")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />

    <button class="btn btn-success">Save changes</button>
</form>
<script>
    function showMessage() {
        alert("Your social media has been changed!");
    }
</script>



